class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        HashMap<Integer,Integer> hm=new HashMap<>();

        List<Integer> ans=new ArrayList<>();

        for(int i=0;i<nums.length;i++)
        {
            if(hm.containsKey(nums[i]))
            {
                ans.add(nums[i]);
            }
            else{
                hm.put(nums[i],1);
            }
        }

        return ans;
    }
}
